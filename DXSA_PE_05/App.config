<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<appSettings>
		<!-- Intervalo de tiempo en milisegundos para el timer (60000 = 1 minuto) -->
		<add key="IntervaloTiempo" value="60000" />
		<!-- Configuración SFTP -->
		<add key="SftpHost" value="localhost" />
		<add key="SftpPuerto" value="2222" />
		<add key="SftpUser" value="alicorp" />
		<add key="SftpPass" value="alicorp123" />
		<!-- Ruta remota en el SFTP donde están los archivos XML -->
		<add key="SftpRutaRemota" value="/home/sftp/alicorp" />
		<!-- Ruta local donde se almacenarán los archivos descargados -->
		<add key="RutaAlmacenar" value="C:\DataExpress\Alicorp\XML" />
		<!-- RUC de la empresa para filtrar carpetas (dejar vacío para todas) -->
		<add key="RucEmpresa" value="" />
	</appSettings>
</configuration>